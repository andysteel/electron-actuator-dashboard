{
  "name": "actuator-dashboard",
  "version": "0.0.1",
  "main": "main.js",
  "engines": {
    "node": ">=10.13.0"
  },
  "repository": "https://github.com/andysteel/electron-actuator-dashboard",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build --base-href ./",
    "build-dev": "npm run build -- -c dev",
    "build-prod": "npm run build -- -c production",
    "electron-serve": "concurrently \"npm start\" \"wait-on tcp:4200 && npm run electron\"",
    "electron-local": "npm run build-prod && electron . --trace-warnings",
    "electron-build": "npm run build-prod && electron-builder build --publish=never",
    "electron": "electron . --serve",
    "electron-linux": "npm run build-prod && electron-builder build --linux",
    "electron-windows": "npm run build-prod && electron-builder build --windows",
    "electron-mac": "npm run build-prod && electron-builder build --mac",
    "doc-electron": "npx compodoc -p src/app/tsconfig.electron.json -d documentation/electron --theme material -s -o"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.9",
    "@angular/cdk": "^11.2.12",
    "@angular/common": "~11.2.9",
    "@angular/compiler": "~11.2.9",
    "@angular/core": "~11.2.9",
    "@angular/forms": "~11.2.9",
    "@angular/platform-browser": "~11.2.9",
    "@angular/platform-browser-dynamic": "~11.2.9",
    "@angular/router": "~11.2.9",
    "@electron/remote": "^1.1.0",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fullcalendar/core": "^4.1.0",
    "@googlemaps/google-maps-services-js": "^3.1.16",
    "@types/quill": "^2.0.6",
    "chart.js": "^2.7.3",
    "ngx-electron": "^2.2.0",
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
    "primeng": "^11.3.2",
    "quill": "^1.3.7",
    "rxjs": "~6.6.0",
    "sqlite": "^4.0.21",
    "sqlite3": "^5.0.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.9",
    "@angular/cli": "^11.2.9",
    "@angular/compiler-cli": "~11.2.9",
    "@compodoc/compodoc": "^1.1.11",
    "@types/chart.js": "^2.9.32",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^14.14.33",
    "codelyzer": "^6.0.0",
    "concurrently": "^6.0.1",
    "electron": "^12.0.9",
    "electron-builder": "^22.10.5",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "^8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5",
    "wait-on": "^5.3.0"
  }
}
